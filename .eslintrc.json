{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "max-len": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "import/no-extraneous-dependencies": "off",
    "class-methods-use-this": "off"
  }
}
